                                Sleep(500)
                            end
                        else
                            FindPath(CONFIG.Fast_plant.horizontal_size[1],CONFIG.Fast_plant.vertical_size[2],100)
                            Sleep(100)
                            place(5640,0,0)
                            Sleep(100)
                            SendPacket(2,[[action|dialog_return
                        dialog_name|cheats
