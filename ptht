                                CONFIG.Fast_plant.coordinate_magplant = LEFT_MAG_X
                                CheckRemote()
                                Sleep(800)
                            end
                            CHANGE_MAGPLANT = false
                        end
                        if GetLocal().pos.y //32 >= vertical_size[1] and GetLocal().pos.y //32 <= vertical_size[2] then
                            if  GetLocal().pos.x //32 >= CONFIG.Fast_plant.horizontal_size[1] or GetLocal().pos.x //32 <= CONFIG.Fast_plant.horizontal_size[2] then
