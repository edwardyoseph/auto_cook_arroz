function FChat(txt)
  p = {}
  p[0] = "OnTextOverlay"
  p[1] = txt
  SendVariantList(p)
end

AddHook("onvariant", "surg", function(var)
if var[0]:find("OnDialogRequest") and var[1]:find("Status: `4Awake(.+)")then
SendPacket(2,[[
action|dialog_return
dialog_name|surgery
buttonClicked|command_2
